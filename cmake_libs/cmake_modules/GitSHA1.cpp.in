#include "cmake_modules/GitSHA1.h"

#define GIT_SHA1 "@GIT_SHA1@"
const char g_GIT_SHA1[] = GIT_SHA1; 
#define GIT_REFSPEC "@GIT_REFSPEC@"
const char g_GIT_REFSPEC[] = GIT_REFSPEC; 
#define GIT_LOCAL_CHANGES "@GIT_LOCAL_CHANGES@"
const char g_GIT_IS_LOCAL_CHANGES[] = GIT_LOCAL_CHANGES; 

std::string g_git_sha1() {
    return std::string(g_GIT_SHA1).substr(0, 7);
}
